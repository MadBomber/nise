%h1 New job
- form_for(@job) do |f|
  = f.error_messages
  %p
    = f.label :name
    %br/
    = f.text_field :name
  %p
    = f.label :description
    %br/
    = f.text_field :description
  %p
    = f.label :default_input_dir
    %br/
    = f.text_field :default_input_dir
  %p
    = f.label :default_output_dir
    %br/
    = f.text_field :default_output_dir
  %p
    = f.submit "Create"
= link_to 'Back', jobs_path
