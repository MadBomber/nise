= form_for(@dispatcher_stat) do |f|
  - if @dispatcher_stat.errors.any?
    #error_explanation
      %h2
        = pluralize(@dispatcher_stat.errors.count, "error")
        prohibited this dispatcher_stat from being saved:

      %ul
        - @dispatcher_stat.errors.full_messages.each do |msg|
          %li= msg %></li>

  .actions
    = f.submit
